ful response\r\n      })\r\n      .catch(error => {\r\n        console.error('Error:', error);  // Handle errors\r\n      });\r\n    },\r\n    // Method to redirect to the menu page\r\n    redirectToMenu() {\r\n      window.location.href = '/menu';\r\n    },\r\n    // Method for handling logout (e.g., clear session, send request to server)\r\n    handleLogout(event) {\r\n      event.preventDefault();  // Prevent default anchor behavior\r\n      axios.post('/logout', {}, { withCredentials: true })\r\n        .then(() => {\r\n          // Redirect to the homepage after logging out\r\n          window.location.href = '/';\r\n        })\r\n        .catch(error => {\r\n          console.error('Logout error:', error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* General reset for padding and margin */\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n/* Navbar styles */\r\n.navbar {\r\n  background-color: #ffe3c5;\r\n  padding: 10px 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.nav-icons {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.cart-icon {\r\n  margin-right: 20px;\r\n  font-size: 1.5em;\r\n  color: #000;\r\n  text-decoration: none;\r\n}\r\n\r\n.cart-icon:hover {\r\n  color: #ff6347; /* Example hover color */\r\n}\r\n\r\n.navbar ul {\r\n  list-style: none;\r\n  display: flex;\r\n  gap: 20px;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.navbar li {\r\n  padding: 8px 16px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.navbar li.active {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n.navbar li:hover {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n.navbar li a {\r\n  text-decoration: none;\r\n  color: #6b4226;\r\n}\r\n\r\n/* Logout Button */\r\n.logout {\r\n  border: none;\r\n  padding: 8px 16px;\r\n  font-size: 1em;\r\n  color: #6b4226;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n  text-decoration: none;\r\n}\r\n\r\n.logout:hover {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Home Section styles */\r\n.home {\r\n  text-align: center;\r\n  padding: 50px;\r\n  color: #6b4226;\r\n}\r\n\r\n.home h1 {\r\n  font-size: 2.5em;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.home p {\r\n  font-size: 1.2em;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n/* Shop Now button */\r\n.shop-now {\r\n  background-color: #ffb7a1;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  font-size: 1em;\r\n  color: white;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.shop-now:hover {\r\n  background-color: #f28268;\r\n}\r\n\r\n/* Responsive design */\r\n@media screen and (max-width: 768px) {\r\n  .navbar ul {\r\n    flex-direction: column;\r\n    align-items: center;\r\n    gap: 10px;\r\n  }\r\n\r\n  .logout {\r\n    margin-top: 10px;\r\n    width: 100%;\r\n    max-width: 200px;\r\n  }\r\n\r\n  .shop-now {\r\n    width: 100%;\r\n    max-width: 200px;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 480px) {\r\n  .home h1 {\r\n    font-size: 1.8em;\r\n  }\r\n\r\n  .home p {\r\n    font-size: 1em;\r\n  }\r\n\r\n  .shop-now {\r\n    font-size: 0.9em;\r\n    padding: 8px 16px;\r\n  }\r\n}\r\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAK;;EAETA,KAAK,EAAC;AAAQ;;EAgBZA,KAAK,EAAC;AAAW;;EASfA,KAAK,EAAC;AAAM;;uBA3BvBC,mBAAA,CAgCM,OAhCNC,UAgCM,GA/BJC,mBAAA,YAAe,EACfC,mBAAA,CAsBM,OAtBNC,UAsBM,G,0BAzBVC,kBAAA,8UAkBMH,mBAAA,iCAAoC,EACpCC,mBAAA,CAKM,OALNG,UAKM,G,0BAJJH,mBAAA,CAEI;IAFDI,IAAI,EAAC,OAAO;IAACR,KAAK,EAAC;MACpBI,mBAAA,CAAoC;IAAjCJ,KAAK,EAAC;EAAsB,IAAMG,mBAAA,2BAA8B,C,sBAErEC,mBAAA,CAA2D;IAAxDI,IAAI,EAAC,GAAG;IAACR,KAAK,EAAC,QAAQ;IAAES,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,YAAA,IAAAD,QAAA,CAAAC,YAAA,IAAAF,IAAA,CAAY;KAAE,QAAM,E,KAI3DR,mBAAA,kBAAqB,EACrBC,mBAAA,CAIU,WAJVU,UAIU,G,0BAHRV,mBAAA,CAA6B,YA