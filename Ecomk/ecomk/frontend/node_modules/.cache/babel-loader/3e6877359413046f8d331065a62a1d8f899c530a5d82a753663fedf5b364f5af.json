se.data);\r\n      } catch (error) {\r\n        console.error('Session not active:', error.response?.data || error);\r\n        this.sessionStatus = false;\r\n      }\r\n    },\r\n\r\n    // Logout method\r\n    logout() {\r\n      // Clear the token and reset data\r\n      this.token = '';\r\n      this.username = '';\r\n      this.email = '';\r\n      this.phone = '';\r\n      // Redirect to the homepage or login page\r\n      window.location.href = '/';\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n/* Navbar styles */\r\n.navbar {\r\n  background-color: #ffe3c5;\r\n  padding: 10px 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.nav-icons {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.cart-icon {\r\n  margin-right: 20px;\r\n  font-size: 1.5em;\r\n  color: #000;\r\n  text-decoration: none;\r\n}\r\n\r\n.cart-icon:hover {\r\n  color: #ff6347;\r\n}\r\n\r\n.navbar ul {\r\n  list-style: none;\r\n  display: flex;\r\n  gap: 20px;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.navbar li {\r\n  padding: 8px 16px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.navbar li:hover {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n.navbar li a {\r\n  text-decoration: none;\r\n  color: #6b4226;\r\n}\r\n\r\n.logout {\r\n  border: none;\r\n  padding: 8px 16px;\r\n  font-size: 1em;\r\n  color: #6b4226;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n  text-decoration: none;\r\n}\r\n\r\n.logout:hover {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n/* User Profile Section styles */\r\n.profile {\r\n  text-align: center;\r\n  padding: 50px;\r\n  color: #6b4226;\r\n}\r\n\r\n.profile h1 {\r\n  font-size: 2.5em;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.profile p {\r\n  font-size: 1.2em;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.profile button {\r\n  background-color: #ffb7a1;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  font-size: 1em;\r\n  color: white;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.profile button:hover {\r\n  background-color: #f28268;\r\n}\r\n\r\n/* Responsive design */\r\n@media screen and (max-width: 768px) {\r\n  .navbar ul {\r\n    flex-direction: column;\r\n    align-items: center;\r\n    gap: 10px;\r\n  }\r\n\r\n  .logout {\r\n    margin-top: 10px;\r\n    width: 100%;\r\n    max-width: 200px;\r\n  }\r\n\r\n  .profile button {\r\n    width: 100%;\r\n    max-width: 200px;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 480px) {\r\n  .profile h1 {\r\n    font-size: 1.8em;\r\n  }\r\n\r\n  .profile p {\r\n    font-size: 1em;\r\n  }\r\n\r\n  .profile button {\r\n    font-size: 0.9em;\r\n    padding: 8px 16px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAc;;EAElBA,KAAK,EAAC;AAAQ;mBAHvB;mBAAA;mBAAA;mBAAA;;EAmBWA,KAAK,EAAC;AAAW;mBAnB5B;mBAAA;oBAAA;;EA6BaA,KAAK,EAAC;AAAS;;uBA5B1BC,mBAAA,CAkCM,OAlCNC,UAkCM,GAjCJC,mBAAA,YAAe,EACfC,mBAAA,CAuBM,OAvBNC,UAuBM,GAtBJD,mBAAA,CAaK,aAZHA,mBAAA,CAEK,aADHA,mBAAA,CAA0C;IAAtCE,IAAI,iBAAiBC,KAAA,CAAAC,KAAK;KAAI,MAAI,iBANhDC,UAAA,E,GAQQL,mBAAA,CAEK,aADHA,mBAAA,CAA0C;IAAtCE,IAAI,iBAAiBC,KAAA,CAAAC,KAAK;KAAI,MAAI,iBAThDE,UAAA,E,GAWQN,mBAAA,CAEK,aADHA,mBAAA,CAAkD;IAA9CE,IAAI,qBAAqBC,KAAA,CAAAC,KAAK;KAAI,UAAQ,iBAZxDG,UAAA,E,GAcQP,mBAAA,CAEK,aADHA,mBAAA,CAAgD;IAA5CE,IAAI,oBAAoBC,KAAA,CAAAC,KAAK;KAAI,SAAO,iBAftDI,UAAA,E,KAkBMT,mBAAA,iCAAoC,EACpCC,mBAAA,CAMM,OANNS,UAMM,GALJT,mBAAA,CAEI;IAFAE,IAAI,iBAAiBC,KAAA,CAAAC,KAAK;IAAIR,KAAK,EAAC;gCACtCI,mBAAA,CAAoC;IAAjCJ,KAAK,EAAC;EAAsB,2B,kBArBzCc,UAAA,GAuBQV,mBAAA,CAA4D;IAAxDE,IAAI,yBAAyBC,KAAA,CAAAC,KAAK;sBAAOD,KAAA,CAAAQ,QAAQ,wBAvB7DC,UAAA,GAwBQZ,mBAAA,CAAuD;IAAnDE,IAAI,aAAaC,KAAA,CAAAC,KAAK;IAAIR,KAAK,EAAC;KAAS,QAAM,iBAxB3DiB,WAAA,E,KA4BId,mBAAA,0BAA6B,EAC7BC,mBAAA,CAKU,WALVc,WAKU,GAJRd,mBAAA,CAAgC,YAA5B,WAAS,GAAAe,gBAAA,CAAGZ,KAAA,CAAAQ,QAAQ,kBACxBX,mBAAA,CA