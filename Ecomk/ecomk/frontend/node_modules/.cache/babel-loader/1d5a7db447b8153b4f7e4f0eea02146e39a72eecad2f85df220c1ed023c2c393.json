chCategories() {\r\n        try {\r\n          const response = await axios.get('http://localhost:5000/api/categories');\r\n          this.categories = response.data;\r\n        } catch (error) {\r\n          console.error('Error fetching categories:', error);\r\n        }\r\n      }\r\n    },\r\n    filters: {\r\n      currency(value) {\r\n        return new Intl.NumberFormat('en-US', {\r\n          style: 'currency',\r\n          currency: 'USD'\r\n        }).format(value);\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .kiosk-container {\r\n    max-width: 800px;\r\n    margin: auto;\r\n    padding: 20px;\r\n  }\r\n  .category {\r\n    margin-bottom: 20px;\r\n  }\r\n  .products {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    gap: 20px;\r\n  }\r\n  .product {\r\n    border: 1px solid #ddd;\r\n    border-radius: 5px;\r\n    padding: 10px;\r\n    width: 150px;\r\n    text-align: center;\r\n  }\r\n  .product img {\r\n    width: 100px;\r\n    height: 100px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAuBE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMN,KAAK,CAACO,GAAG,CAAC,sCAAsC,CAAC;QACxE,IAAI,CAACL,UAAS,GAAII,QAAQ,CAACL,IAAI;MACjC,EAAE,OAAOO,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD;IACF;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,QAAQA,CAACC,KAAK,EAAE;MACd,OAAO,IAAIC,IAAI,CAACC,YAAY,CAAC,OAAO,EAAE;QACpCC,KAAK,EAAE,UAAU;QACjBJ,QAAQ,EAAE;MACZ,CAAC,CAAC,CAACK,MAAM,CAACJ,KAAK,CAAC;IAClB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}