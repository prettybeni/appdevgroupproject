{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      sessionStatus: null,\n      // null: checking, true: session active, false: session not active\n      username: '',\n      // Store username here\n      email: '',\n      // Store email here\n      phone: '',\n      // Store phone here\n      token: '' // Store token here\n    };\n  },\n  created() {\n    this.getTokenFromUrl();\n  },\n  mounted() {\n    if (this.token) {\n      this.checkSession(this.token); // Use the token to check the session\n    }\n  },\n  methods: {\n    // Extract token from URL\n    getTokenFromUrl() {\n      const urlParams = new URLSearchParams(window.location.search);\n      this.token = urlParams.get('token');\n      console.log('Received token:', this.token);\n    },\n    // Function to check session using the token\n    async checkSession(token) {\n      if (!token) {\n        console.log(\"No token provided\");\n        this.sessionStatus = false;\n        return;\n      }\n      try {\n        // Wait for the response from the API\n        const response = await axios.get('http://localhost:5000/api/user-profile', {\n          params: {\n            token: token\n          },\n          // Send token as a query parameter\n          withCredentials: true\n        });\n\n        // Check if response contains the expected data\n        console.log('Fetched user data:', response.data);\n\n        // Set session status to active and update user data\n        this.sessionStatus = true;\n        this.username = response.data.user.username;\n        this.email = response.data.user.email;\n        this.phone = response.data.user.phone;\n\n        // Log the fetched data for debugging\n        console.log('User data:', this.username, this.email, this.phone);\n      } catch (error) {\n        console.error('Session not active:', error.response?.data || error);\n        this.sessionStatus = false;\n      }\n    },\n    // Logout method\n    logout() {\n      // Clear the token and reset data\n      this.token = '';\n      this.username = '';\n      this.email = '';\n      this.phone = '';\n      // Redirect to the homepage or login page\n      window.location.href = '/';\n    }\n  }\n};","map":{"version":3,"names":["axios","data","sessionStatus","username","email","phone","token","created","getTokenFromUrl","mounted","checkSession","methods","urlParams","URLSearchParams","window","location","search","get","console","log","response","params","withCredentials","user","error","logout","href"],"sources":["C:\\Users\\norma\\project\\ecomk\\frontend\\src\\views\\userProfile.vue"],"sourcesContent":["<template>\r\n  <div class=\"user-profile\">\r\n    <!-- Navbar -->\r\n    <nav class=\"navbar\">\r\n      <ul>\r\n        <li>\r\n          <a :href=\"`/home?token=${token}`\">Home</a>\r\n        </li>\r\n        <li>\r\n          <a :href=\"`/menu?token=${token}`\">Menu</a>\r\n        </li>\r\n        <li>\r\n          <a :href=\"`/about-us?token=${token}`\">About Us</a>\r\n        </li>\r\n        <li>\r\n          <a :href=\"`/contact?token=${token}`\">Contact</a>\r\n        </li>\r\n      </ul>\r\n      <!-- Cart Icon and Logout Button -->\r\n      <div class=\"nav-icons\">\r\n        <a :href=\"`/cart?token=${token}`\" class=\"cart-icon\">\r\n          <i class=\"fas fa-shopping-cart\"></i>\r\n        </a>\r\n        <a :href=\"`/user-profile?token=${token}`\">{{ username }}</a>\r\n        <a :href=\"`/?token=${token}`\" class=\"logout\">Logout</a>\r\n      </div>\r\n    </nav>\r\n\r\n    <!-- User Profile Section -->\r\n    <section class=\"profile\" v-if=\"sessionStatus === true\">\r\n      <h1>Welcome, {{ username }}</h1>\r\n      <p>Email: {{ email }}</p>\r\n      <p>Phone: {{ phone }}</p>\r\n      <button @click=\"logout\">Logout</button>\r\n    </section>\r\n\r\n    <!-- Loading or Error state -->\r\n    <section v-else-if=\"sessionStatus === null\">\r\n      <p>Loading...</p>\r\n    </section>\r\n    <section v-else>\r\n      <p>Session expired or invalid token. Please log in again.</p>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      sessionStatus: null, // null: checking, true: session active, false: session not active\r\n      username: '', // Store username here\r\n      email: '', // Store email here\r\n      phone: '', // Store phone here\r\n      token: '', // Store token here\r\n    };\r\n  },\r\n  created() {\r\n    this.getTokenFromUrl();\r\n  },\r\n  mounted() {\r\n    if (this.token) {\r\n      this.checkSession(this.token); // Use the token to check the session\r\n    }\r\n  },\r\n  methods: {\r\n    // Extract token from URL\r\n    getTokenFromUrl() {\r\n      const urlParams = new URLSearchParams(window.location.search);\r\n      this.token = urlParams.get('token');\r\n      console.log('Received token:', this.token);\r\n    },\r\n\r\n    // Function to check session using the token\r\n    async checkSession(token) {\r\n      if (!token) {\r\n        console.log(\"No token provided\");\r\n        this.sessionStatus = false;\r\n        return;\r\n      }\r\n\r\n      try {\r\n        // Wait for the response from the API\r\n        const response = await axios.get('http://localhost:5000/api/user-profile', {\r\n          params: { token: token }, // Send token as a query parameter\r\n          withCredentials: true,\r\n        });\r\n\r\n        // Check if response contains the expected data\r\n        console.log('Fetched user data:', response.data);\r\n\r\n        // Set session status to active and update user data\r\n        this.sessionStatus = true;\r\n        this.username = response.data.user.username;\r\nthis.email = response.data.user.email;\r\nthis.phone = response.data.user.phone;\r\n\r\n\r\n        // Log the fetched data for debugging\r\n        console.log('User data:', this.username, this.email, this.phone);\r\n      } catch (error) {\r\n        console.error('Session not active:', error.response?.data || error);\r\n        this.sessionStatus = false;\r\n      }\r\n    },\r\n\r\n    // Logout method\r\n    logout() {\r\n      // Clear the token and reset data\r\n      this.token = '';\r\n      this.username = '';\r\n      this.email = '';\r\n      this.phone = '';\r\n      // Redirect to the homepage or login page\r\n      window.location.href = '/';\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n/* Navbar styles */\r\n.navbar {\r\n  background-color: #ffe3c5;\r\n  padding: 10px 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.nav-icons {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.cart-icon {\r\n  margin-right: 20px;\r\n  font-size: 1.5em;\r\n  color: #000;\r\n  text-decoration: none;\r\n}\r\n\r\n.cart-icon:hover {\r\n  color: #ff6347;\r\n}\r\n\r\n.navbar ul {\r\n  list-style: none;\r\n  display: flex;\r\n  gap: 20px;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.navbar li {\r\n  padding: 8px 16px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.navbar li:hover {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n.navbar li a {\r\n  text-decoration: none;\r\n  color: #6b4226;\r\n}\r\n\r\n.logout {\r\n  border: none;\r\n  padding: 8px 16px;\r\n  font-size: 1em;\r\n  color: #6b4226;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n  text-decoration: none;\r\n}\r\n\r\n.logout:hover {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n/* User Profile Section styles */\r\n.profile {\r\n  text-align: center;\r\n  padding: 50px;\r\n  color: #6b4226;\r\n}\r\n\r\n.profile h1 {\r\n  font-size: 2.5em;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.profile p {\r\n  font-size: 1.2em;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.profile button {\r\n  background-color: #ffb7a1;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  font-size: 1em;\r\n  color: white;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.profile button:hover {\r\n  background-color: #f28268;\r\n}\r\n\r\n/* Responsive design */\r\n@media screen and (max-width: 768px) {\r\n  .navbar ul {\r\n    flex-direction: column;\r\n    align-items: center;\r\n    gap: 10px;\r\n  }\r\n\r\n  .logout {\r\n    margin-top: 10px;\r\n    width: 100%;\r\n    max-width: 200px;\r\n  }\r\n\r\n  .profile button {\r\n    width: 100%;\r\n    max-width: 200px;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 480px) {\r\n  .profile h1 {\r\n    font-size: 1.8em;\r\n  }\r\n\r\n  .profile p {\r\n    font-size: 1em;\r\n  }\r\n\r\n  .profile button {\r\n    font-size: 0.9em;\r\n    padding: 8px 16px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;AA+CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,IAAI;MAAE;MACrBC,QAAQ,EAAE,EAAE;MAAE;MACdC,KAAK,EAAE,EAAE;MAAE;MACXC,KAAK,EAAE,EAAE;MAAE;MACXC,KAAK,EAAE,EAAE,CAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,IAAI,CAACH,KAAK,EAAE;MACd,IAAI,CAACI,YAAY,CAAC,IAAI,CAACJ,KAAK,CAAC,EAAE;IACjC;EACF,CAAC;EACDK,OAAO,EAAE;IACP;IACAH,eAAeA,CAAA,EAAG;MAChB,MAAMI,SAAQ,GAAI,IAAIC,eAAe,CAACC,MAAM,CAACC,QAAQ,CAACC,MAAM,CAAC;MAC7D,IAAI,CAACV,KAAI,GAAIM,SAAS,CAACK,GAAG,CAAC,OAAO,CAAC;MACnCC,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAE,IAAI,CAACb,KAAK,CAAC;IAC5C,CAAC;IAED;IACA,MAAMI,YAAYA,CAACJ,KAAK,EAAE;MACxB,IAAI,CAACA,KAAK,EAAE;QACVY,OAAO,CAACC,GAAG,CAAC,mBAAmB,CAAC;QAChC,IAAI,CAACjB,aAAY,GAAI,KAAK;QAC1B;MACF;MAEA,IAAI;QACF;QACA,MAAMkB,QAAO,GAAI,MAAMpB,KAAK,CAACiB,GAAG,CAAC,wCAAwC,EAAE;UACzEI,MAAM,EAAE;YAAEf,KAAK,EAAEA;UAAM,CAAC;UAAE;UAC1BgB,eAAe,EAAE;QACnB,CAAC,CAAC;;QAEF;QACAJ,OA