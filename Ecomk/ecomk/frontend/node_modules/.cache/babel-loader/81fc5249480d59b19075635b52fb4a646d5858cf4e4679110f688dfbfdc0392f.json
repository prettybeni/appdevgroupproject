{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  name: 'ManageProducts',\n  data() {\n    return {\n      products: [] // Array to hold product data\n    };\n  },\n  mounted() {\n    this.fetchProducts();\n  },\n  methods: {\n    async fetchProducts() {\n      try {\n        const response = await fetch('/api/products'); // Matches the route\n        if (response.ok) {\n          this.products = await response.json();\n        } else {\n          console.error('Failed to fetch products');\n        }\n      } catch (error) {\n        console.error('Error fetching products:', error);\n      }\n    },\n    async deleteProduct(productId) {\n      try {\n        const response = await fetch(`/api/products/${productId}`, {\n          method: 'DELETE'\n        });\n        if (response.ok) {\n          this.products = this.products.filter(product => product.id !== productId);\n        } else {\n          alert('Failed to delete product');\n        }\n      } catch (error) {\n        console.error('Error deleting product:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","products","mounted","fetchProducts","methods","response","fetch","ok","json","console","error","deleteProduct","productId","method","filter","product","id","alert"],"sources":["C:\\Users\\norma\\project\\ecomk\\frontend\\src\\views\\Manage.vue"],"sourcesContent":["<template>\r\n    <div class=\"manage-products\">\r\n      <h1>Manage Products</h1>\r\n      <ul v-if=\"products.length\">\r\n        <li v-for=\"product in products\" :key=\"product.id\">\r\n          <h3>{{ product.name }}</h3>\r\n          <p>Price: {{ product.price }}</p>\r\n          <p>{{ product.description }}</p>\r\n          <button @click=\"editProduct(product.id)\">Edit</button>\r\n          <button @click=\"deleteProduct(product.id)\">Delete</button>\r\n        </li>\r\n      </ul>\r\n      <p v-else>No products available</p>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    name: 'ManageProducts',\r\n    data() {\r\n      return {\r\n        products: [] // Array to hold product data\r\n      };\r\n    },\r\n    mounted() {\r\n      this.fetchProducts();\r\n    },\r\n    methods: {\r\n  async fetchProducts() {\r\n    try {\r\n      const response = await fetch('/api/products'); // Matches the route\r\n      if (response.ok) {\r\n        this.products = await response.json();\r\n      } else {\r\n        console.error('Failed to fetch products');\r\n      }\r\n    } catch (error) {\r\n      console.error('Error fetching products:', error);\r\n    }\r\n  },\r\n  async deleteProduct(productId) {\r\n    try {\r\n      const response = await fetch(`/api/products/${productId}`, { method: 'DELETE' });\r\n      if (response.ok) {\r\n        this.products = this.products.filter(product => product.id !== productId);\r\n      } else {\r\n        alert('Failed to delete product');\r\n      }\r\n    } catch (error) {\r\n      console.error('Error deleting product:', error);\r\n    }\r\n  }\r\n}\r\n\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .manage-products {\r\n    padding: 20px;\r\n  }\r\n  \r\n  ul {\r\n    list-style-type: none;\r\n    padding: 0;\r\n  }\r\n  \r\n  li {\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  button {\r\n    margin-right: 10px;\r\n    padding: 5px 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":";;AAiBE,eAAe;EACbA,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAC,CAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACX,MAAMD,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMC,KAAK,CAAC,eAAe,CAAC,EAAE;QAC/C,IAAID,QAAQ,CAACE,EAAE,EAAE;UACf,IAAI,CAACN,QAAO,GAAI,MAAMI,QAAQ,CAACG,IAAI,CAAC,CAAC;QACvC,OAAO;UACLC,OAAO,CAACC,KAAK,CAAC,0BAA0B,CAAC;QAC3C;MACF,EAAE,OAAOA,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF,CAAC;I