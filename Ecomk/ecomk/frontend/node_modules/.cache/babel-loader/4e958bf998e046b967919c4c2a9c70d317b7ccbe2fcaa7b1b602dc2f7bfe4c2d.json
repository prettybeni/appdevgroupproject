{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      categories: [],\n      products: [],\n      currentView: 'categories' // Options: 'categories', 'products'\n    };\n  },\n  created() {\n    this.fetchCategories();\n    this.fetchProducts();\n  },\n  methods: {\n    async fetchCategories() {\n      try {\n        const response = await axios.get('http://localhost:5000/api/categories');\n        this.categories = response.data;\n      } catch (error) {\n        console.error(\"Error fetching categories:\", error);\n      }\n    },\n    async fetchProducts() {\n      try {\n        const response = await axios.get('http://localhost:5000/api/products');\n        this.products = response.data;\n      } catch (error) {\n        console.error(\"Error fetching products:\", error);\n      }\n    },\n    viewCategories() {\n      this.currentView = 'categories';\n    },\n    viewProducts() {\n      this.currentView = 'products';\n    },\n    addProduct() {\n      this.$router.push('/admin/add-product');\n    },\n    editCategory(id) {\n      this.$router.push(`/admin/edit-category/${id}`);\n    },\n    deleteCategory(id) {\n      // Functionality for deleting a category can be added here\n      console.log(`Delete category with ID: ${id}`);\n    },\n    editProduct(id) {\n      this.$router.push(`/admin/edit-product/${id}`);\n    },\n    deleteProduct(id) {\n      // Functionality for deleting a product can be added here\n      console.log(`Delete product with ID: ${id}`);\n    }\n  },\n  filters: {\n    currency(value) {\n      return new Intl.NumberFormat('en-US', {\n        style: 'currency',\n        currency: 'USD'\n      }).format(value);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","categories","products","currentView","created","fetchCategories","fetchProducts","methods","response","get","error","console","viewCategories","viewProducts","addProduct","$router",