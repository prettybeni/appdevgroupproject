{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport axios from 'axios';\nexport default {\n  props: ['orderItems'],\n  computed: {\n    totals() {\n      let totals = {\n        beverages: 0,\n        desserts: 0,\n        addOns: 0,\n        overall: 0\n      };\n      this.orderItems.forEach(item => {\n        if (item.category === \"Beverages\") totals.beverages += item.price * item.quantity;\n        if (item.category === \"Desserts\") totals.desserts += item.price * item.quantity;\n        if (item.category === \"Add Ons\") totals.addOns += item.price * item.quantity;\n        totals.overall += item.price * item.quantity;\n      });\n      return totals;\n    }\n  },\n  methods: {\n    removeItem(index) {\n      this.$emit(\"remove-item\", index);\n    },\n    async placeOrder() {\n      try {\n        const payload = {\n          items: this.orderItems,\n          totalPrice: this.totals.overall\n        };\n        await axios.post(\"http://localhost:5000/api/orders\", payload);\n        alert(\"Order placed successfully!\");\n        this.$emit(\"clear-order\");\n      } catch (error) {\n        console.error(\"Error placing order:\", error);\n        alert(\"Failed to place the order. Please try again.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","computed","totals","beverages","desserts","addOns","overall","orderItems","forEach","item","category","price","quantity","methods","removeItem","index","$emit","placeOrder","payload","items","totalPrice","post","alert","error","console"],"sources":["C:\\Users\\norma\\project\\ecomk\\frontend\\src\\components\\OrderSummary.vue"],"sourcesContent":["<template>\r\n  <div class=\"order-summary\">\r\n    <h3>Order Summary</h3>\r\n    <ul>\r\n      <li v-for=\"(item, index) in orderItems\" :key=\"index\">\r\n        <span>{{ item.name }} ({{ item.quantity }}x) - {{ item.price * item.quantity }} Php</span>\r\n        <!-- Trash Bin Icon to Remove Item -->\r\n        <button class=\"remove-item-btn\" @click=\"removeItem(index)\">\r\n          <i class=\"fas fa-trash-alt\"></i> <!-- Font Awesome Trash Bin Icon -->\r\n        </button>\r\n      </li>\r\n    </ul>\r\n    <div class=\"totals\">\r\n      <p>Beverages Total: {{ totals.beverages }} Php</p>\r\n      <p>Desserts Total: {{ totals.desserts }} Php</p>\r\n      <p>Add Ons Total: {{ totals.addOns }} Php</p>\r\n      <hr />\r\n      <p>Total: {{ totals.overall }} Php</p>\r\n    </div>\r\n    <button @click=\"placeOrder\" :disabled=\"orderItems.length === 0\">Place Order</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: ['orderItems'],\r\n  computed: {\r\n    totals() {\r\n      let totals = {\r\n        beverages: 0,\r\n        desserts: 0,\r\n        addOns: 0,\r\n        overall: 0,\r\n      };\r\n\r\n      this.orderItems.forEach((item) => {\r\n        if (item.category === \"Beverages\") totals.beverages += item.price * item.quantity;\r\n        if (item.category === \"Desserts\") totals.desserts += item.price * item.quantity;\r\n        if (item.category === \"Add Ons\") totals.addOns += item.price * item.quantity;\r\n        totals.overall += item.price * item.quantity;\r\n      });\r\n\r\n      return totals;\r\n    },\r\n  },\r\n  methods: {\r\n    removeItem(index) {\r\n      this.$emit(\"remove-item\", index);\r\n    },\r\n    async placeOrder() {\r\n      try {\r\n        const payload = {\r\n          items: this.orderItems,\r\n          totalPrice: this.totals.overall,\r\n        };\r\n        await axios.post(\"http://localhost:5000/api/orders\", payload);\r\n        alert(\"Order placed successfully!\");\r\n        this.$emit(\"clear-order\");\r\n      } catch (error) {\r\n        console.error(\"Error placing order:\", error);\r\n        alert(\"Failed to place the order. Please try again.\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.order-summary ul {\r\n  padding: 0;\r\n  margin: 0;\r\n  max-height: 200px; /* Set a fixed height for the list */\r\n  overflow-y: auto; /* Enable vertical scrolling */\r\n}\r\n\r\n.order-summary li {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 5px 0; /* Reduced padding to decrease space between products */\r\n  border-bottom: 1px solid #ddd; /* Optional, if you want a line between items */\r\n}\r\n\r\n.remove-item-btn {\r\n  background: none;\r\n  border: none;\r\n  color: #dc3545;\r\n  font-size: 1.2rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.remove-item-btn:hover {\r\n  color: #c82333;\r\n}\r\n\r\n.totals {\r\n  margin-top: 1rem;\r\n}\r\n\r\nbutton {\r\n  margin-top: 1rem;\r\n  padding: 0.5rem 1rem;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 4px;\r\n}\r\n\r\nbutton:disabled {\r\n  background-color: #ccc;\r\n}\r\n</style>\r\n"],"mappings":";;AAwBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE,CAAC,YAAY,CAAC;EACrBC,QAAQ,EAAE;IACRC,MAAMA,CAAA,EAAG;MACP,IAAIA,MAAK,GAAI;QACXC,SAAS,EAAE,CAAC;QACZC,QAAQ,EAAE,CAAC;QACXC,MAAM,EAAE,CAAC;QACTC,OAAO,EAAE;MACX,CAAC;MAED,IAAI,CAACC,UAAU,CAACC,OAAO,CAAEC,IAAI,IAAK;QAChC,IAAIA,IAAI,CAACC,QAAO,KAAM,WAAW,EAAER,MAAM,CAACC,SAAQ,IAAKM,IAAI,CAACE,KAAI,GAAIF,IAAI,CAACG,QAAQ;QACjF,IAAIH,IAAI,CAACC,QAAO,KAAM,UAAU,EAAER,MAAM,CAACE,QAAO,IAAKK,IAAI,CAACE,KAAI,GAAIF,IAAI,CAACG,QAAQ;QAC/E,IAAIH,IAAI,CAACC,QAAO,KAAM,SAAS,EAAER,MAAM,CAACG,MAAK,IAAKI,IAAI,CAACE,KAAI,GAAIF,IAAI,CAACG,QAAQ;QAC5EV,MAAM,CAACI,OAAM,IAAKG,IAAI,CAACE,KAAI,GAAIF,IAAI,CAACG,QAAQ;MAC9C,CAAC,CAAC;MAEF,OAAOV,MAAM;IACf;EACF,CAAC;EACDW,OAAO,EAAE;IACPC,UAAUA,CAACC,KAAK,EAAE;MAChB,IAAI,CAACC,KAAK,CAAC,aAAa,EAAED,KAAK,CAAC;IAClC,CAAC;IACD,MAAME,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,OAAM,GAAI;UACdC,KAAK,EAAE,IAAI,CAACZ,UAAU;UACtBa,UAAU,EAAE,IAAI,CAAClB,MAAM,CAACI;QAC1B,CAAC;QACD,MAAMP,KAAK,CAACsB,IAAI,CAAC,kCAAkC,EAAEH,OAAO,CAAC;QAC7DI,KAAK,CAAC,4BAA4B,CA