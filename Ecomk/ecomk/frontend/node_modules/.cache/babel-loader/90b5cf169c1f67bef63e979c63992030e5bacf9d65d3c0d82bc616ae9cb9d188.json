r (reused from Home.vue) -->\r\n    <nav class=\"navbar\">\r\n      <ul>\r\n        <li class=\"active\">\r\n          <a href=\"/home\">Home</a>\r\n        </li>\r\n        <li>\r\n          <a href=\"/menu\">Menu</a>\r\n        </li>\r\n        <li>\r\n          <a href=\"/about-us\">About Us</a>\r\n        </li>\r\n        <li>\r\n          <a href=\"/contact\">Contact</a>\r\n        </li>\r\n      </ul>\r\n      <!-- Logout Button -->\r\n      <div>\r\n        <a href=\"/\" class=\"logout\">Logout</a>\r\n      </div>\r\n    </nav>\r\n\r\n    <!-- Menu Section -->\r\n    <section class=\"menu-section\">\r\n      <h1>Our Menu</h1>\r\n      \r\n      <!-- Categories -->\r\n      <div class=\"categories\">\r\n        <div v-for=\"category in categories\" :key=\"category.id\" class=\"category-card\">\r\n          <h2>{{ category.name }}</h2>\r\n          <div class=\"products\">\r\n            <div v-for=\"product in filteredProducts(category.id)\" :key=\"product.id\" class=\"product-card\">\r\n              <img :src=\"product.image_url\" alt=\"product image\" class=\"product-img\" />\r\n              <h3>{{ product.name }}</h3>\r\n              <p>{{ product.description }}</p>\r\n              <span class=\"price\">${{ product.price }}</span>\r\n              <button class=\"add-to-cart\" @click=\"addToCart(product)\">Add to Cart</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      categories: [\r\n        { id: 1, name: \"Milk Teas\" },\r\n        { id: 2, name: \"Fruit Teas\" },\r\n        { id: 3, name: \"Smoothies\" },\r\n        { id: 4, name: \"Snacks\" },\r\n      ],\r\n      products: [\r\n        { id: 1, name: \"Classic Milk Tea\", category_id: 1, price: 5, image_url: \"/images/milk-tea.jpg\", description: \"A traditional milk tea.\" },\r\n        { id: 2, name: \"Mango Fruit Tea\", category_id: 2, price: 