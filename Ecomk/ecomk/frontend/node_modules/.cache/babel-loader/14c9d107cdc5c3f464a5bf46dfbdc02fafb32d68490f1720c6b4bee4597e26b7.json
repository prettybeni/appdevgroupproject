       <a href=\"/\" class=\"logout\">Logout</a>\r\n      </div>\r\n    </nav>\r\n\r\n    <!-- Menu Section -->\r\n    <section class=\"menu-section\">\r\n      <h1>Our Menu</h1>\r\n      \r\n      <!-- Categories -->\r\n      <div class=\"categories\">\r\n        <div v-for=\"category in categories\" :key=\"category.id\" class=\"category-card\">\r\n          <h2>{{ category.name }}</h2>\r\n          <div class=\"products\">\r\n            <div v-for=\"product in filteredProducts(category.id)\" :key=\"product.id\" class=\"product-card\">\r\n              <img :src=\"product.image_url\" alt=\"product image\" class=\"product-img\" />\r\n              <h3>{{ product.name }}</h3>\r\n              <p>{{ product.description }}</p>\r\n              <span class=\"price\">${{ product.price }}</span>\r\n              <button class=\"add-to-cart\" @click=\"addToCart(product)\">Add to Cart</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      categories: [\r\n        { id: 1, name: \"Milk Teas\" },\r\n        { id: 2, name: \"Fruit Teas\" },\r\n        { id: 3, name: \"Smoothies\" },\r\n        { id: 4, name: \"Snacks\" },\r\n      ],\r\n      products: [\r\n        { id: 1, name: \"Classic Milk Tea\", category_id: 1, price: 5, image_url: \"/images/milk-tea.jpg\", description: \"A traditional milk tea.\" },\r\n        { id: 2, name: \"Mango Fruit Tea\", category_id: 2, price: 6, image_url: \"/images/fruit-tea.jpg\", description: \"Refreshing mango tea.\" },\r\n        { id: 3, name: \"Strawberry Smoothie\", category_id: 3, price: 7, image_url: \"/images/smoothie.jpg\", description: \"Smooth and creamy strawberry smoothie.\" },\r\n        { id: 4, name: \"Cheese Fries\", category_id: 4, price: 4, image_url: \"/images/snacks.jpg\", description: \"Crispy fries with cheese.\" },\r\n        // More products can be added\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    // Filters products by category\r\n    filteredProducts(categoryId) {\r\n      return this.products.filter(product => product.category_id === categoryId);\r\n    },\r\n    addToCart(product) {\r\n      // Handle adding product to the cart\r\n      alert(`${product.name} added to cart!`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* General reset for padding and margin */\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n/* Navbar styles (same as home) */\r\n.navbar {\r\n  background-color: #ffe3c5;\r\n  padding: 10px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.navbar ul {\r\n  list-style: none;\r\n  display: flex;\r\n  gap: 20px;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.navbar li {\r\n  padding: 8px 16px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.navbar li.active {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n.navbar li a {\r\n  text-decoration: none;\r\n  color: #6b4226;\r\n}\r\n\r\n/* Logout Button */\r\n.logout {\r\n  border: none;\r\n  padding: 8px 16px;\r\n  font-size: 1em;\r\n  color: #6b4226;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n  text-decoration: none;\r\n}\r\n\r\n.logout:hover {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Menu Section styles */\r\n.menu-section {\r\n  padding: 50px;\r\n  text-align: center;\r\n  background-color: #fffaf0;\r\n  color: #6b4226;\r\n}\r\n\r\n.menu-section h1 {\r\n  font-size: 2.5em;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n/* Category Card Styles */\r\n.categories {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 30px;\r\n  justify-content: center;\r\n}\r\n\r\n.category-card {\r\n  width: 300px;\r\n  background-color: #ffefdc;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.category-card h2 {\r\n  font-size: 1.8em;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* Product Card Styles */\r\n.products {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n  justify-content: center;\r\n}\r\n\r\n.product-card {\r\n  width: 200px;\r\n  background-color: #fff;\r\n  padding: 15px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  text-align: center;\r\n}\r\n\r\n.product-card img {\r\n  width: 100%;\r\n  height: auto;\r\n  border-radius: 8px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.product-card h3 {\r\n  font-size: 1.2em;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.product-card p {\r\n  font-size: 1em;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.product-card .price {\r\n  font-size: 1.2em;\r\n  color: #ff6f61;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.product-card .add-to-cart {\r\n  background-color: #ffb7a1;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  font-size: 1em;\r\n  color: white;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.product-card .add-to-cart:hover {\r\n  background-color: #f28268;\r\n}\r\n\r\n/* Responsive design */\r\n@media screen and (max-width: 768px) {\r\n  .navbar ul {\r\n    flex-direction: column;\r\n    align-items: center;\r\n    gap: 10px;\r\n  }\r\n\r\n  .logout {\r\n    margin-top: 10px;\r\n    width: 100%;\r\n    max-width: 200px;\r\n  }\r\n\r\n  .product-card {\r\n    width: 100%;\r\n    max-width: 250px;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 480px) {\r\n  .menu-section h1 {\r\n    font-size: 1.8em;\r\n  }\r\n\r\n  .product-card h3 {\r\n    font-size: 1em;\r\n  }\r\n\r\n  .product-card .price {\r\n    font-size: 1em;\r\n  }\r\n\r\n  .product-card .add-to-cart {\r\n    font-size: 0.9em;\r\n    padding: 8px 16px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAM;;EAwBNA,KAAK,EAAC;AAAc;;EAItBA,KAAK,EAAC;AAAY;;EAGdA,KAAK,EAAC;AAAU;mBAhC/B;;EAqCoBA,KAAK,EAAC;AAAO;mBArCjC;;uBACEC,mBAAA,CA2CM,OA3CNC,UA