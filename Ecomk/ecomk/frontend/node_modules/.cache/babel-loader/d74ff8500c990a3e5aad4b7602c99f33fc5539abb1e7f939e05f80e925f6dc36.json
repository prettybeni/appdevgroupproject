/ Handle the form submission logic for adding an ingredient\r\n        console.log(\"Adding ingredient:\", this.ingredient);\r\n        // Logic for saving the ingredient (e.g., API call) would go here\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Root container */\r\n  .admin-dashboard {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    width: 100%;\r\n  }\r\n  \r\n  /* Navigation Bar */\r\n  .navbar {\r\n    width: 100%;\r\n    padding: 22px 0;\r\n    background-color: #f5a8a0; /* Darker pink background */\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    position: relative;\r\n    z-index: 2;\r\n  }\r\n  \r\n  /* Hamburger Menu */\r\n  .hamburger-menu {\r\n    display: block;\r\n    cursor: pointer;\r\n    font-size: 24px;\r\n    color: #6b4226;\r\n    position: absolute;\r\n    left: 20px;\r\n    z-index: 3;\r\n  }\r\n  \r\n  /* Admin Dashboard Text */\r\n  .admin-dashboard-text {\r\n    font-size: 20px;\r\n    font-weight: bold;\r\n    color: #6b4226;\r\n    flex-grow: 1;\r\n    text-align: center;\r\n    z-index: 2;\r\n  }\r\n  \r\n  /* Navigation Links */\r\n  .nav-links {\r\n    display: flex;\r\n    flex-direction: column;\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 300px;\r\n    height: 100%;\r\n    background-color: #ffe3c5;\r\n    transform: translateX(-100%);\r\n    transition: transform 0.5s ease-in-out;\r\n    z-index: 1;\r\n    padding-top: 82px;\r\n  }\r\n  \r\n  .nav-links.active {\r\n    transform: translateX(0);\r\n  }\r\n  \r\n  /* Dropdown Button Active State */\r\n  .active-btn {\r\n    background-color: #ffb7a1;\r\n  }\r\n  \r\n  /* Active Link */\r\n  .active-link {\r\n    background-color: #ffb7a1;\r\n  }\r\n  \r\n  /* Dropdown Content */\r\n  .dropdown-content {\r\n    display: none;\r\n    background: #fffaf0;\r\n    width: 100%;\r\n    box-sizing: border-box;\r\n  }\r\n  \r\n  .dropdown-content a {\r\n    color: #6b4226;\r\n    font-size: 16px;\r\n    padding: 12px 20px;\r\n    display: block;\r\n    text-decoration: none;\r\n    width: 100%;\r\n    box-sizing: border-box;\r\n    transition: background-color 0.3s ease;\r\n  }\r\n  \r\n  .dropdown-content a:hover {\r\n    background-color: #ffb7a1;\r\n  }\r\n  \r\n  /* Display the dropdown content when active */\r\n  .nav-dropdown .dropdown-content {\r\n    display: block;\r\n  }\r\n  \r\n  /* Individual Links */\r\n  .nav-link {\r\n    background-color: #ffe3c5;\r\n    color: #6b4226;\r\n    font-size: 16px;\r\n    padding: 12px 20px;\r\n    width: 100%;\r\n    text-align: left;\r\n    cursor: pointer;\r\n    display: block;\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .nav-link:hover {\r\n    background-color: #ffb7a1;\r\n  }\r\n  \r\n  /* Add Ingredient Form */\r\n  .dashboard-content {\r\n    width: 100%;\r\n    padding: 20px;\r\n    box-sizing: border-box;\r\n  }\r\n  \r\n  h2 {\r\n    margin-bottom: 20px;\r\n    color: #6b4226;\r\n  }\r\n  \r\n  label {\r\n    display: block;\r\n    margin-bottom: 8px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  input, textarea {\r\n    width: 100%;\r\n    padding: 10px;\r\n    margin-bottom: 20px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 4px;\r\n    box-sizing: border-box;\r\n  }\r\n  \r\n  button.add-ingredient-btn {\r\n    background-color: #f5a8a0;\r\n    color: white;\r\n    font-size: 16px;\r\n    font-weight: bold;\r\n    padding: 12px 20px;\r\n    border: none;\r\n    cursor: pointer;\r\n    width: 100%;\r\n    text-align: center;\r\n  }\r\n  \r\n  button.add-ingredient-btn:hover {\r\n    background-color: #ffb7a1;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAoFE,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,KAAK;MACjBC,cAAc,EAAE,IAAI;MACpBC,UAAU,EAAE;QACVJ,IAAI,EAAE,EAAE;QACRK,WAAW,EAAE;MACf,CAAC;MACDC,mBAAmB,EAAE,IAAI,CAAE;IAC7B,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACN,UAAS,GAAI,CAAC,IAAI,CAACA,U