a v-else href=\"/login\" class=\"logout\">Login</a>\r\n      </div>\r\n    </nav>\r\n\r\n    <!-- Home Section -->\r\n    <section class=\"home\">\r\n      <h1>Welcome to Tea Haven</h1>\r\n      <p>Your favorite milk tea, delivered to your doorstep!</p>\r\n      <button class=\"shop-now\" @click=\"goToMenu\">Shop Now</button>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      sessionActive: false, // Track if session is active\r\n      username: '',         // Store username\r\n    };\r\n  },\r\n  created() {\r\n    this.checkSession();\r\n  },\r\n  methods: {\r\n    // Function to check session\r\n    checkSession() {\r\n      const token = this.$route.query.token; // Get the token from URL query params\r\n\r\n      if (!token) {\r\n        console.error('Token is missing');\r\n        this.sessionActive = false;\r\n        return;\r\n      }\r\n\r\n      axios.get(`http://localhost:5000/api/homepage?token=${token}`, { withCredentials: true })\r\n        .then((response) => {\r\n          this.sessionActive = true;\r\n          this.username = response.data.username; // Assuming the backend sends the username\r\n        })\r\n        .catch(error => {\r\n          console.error('Session not active:', error.response?.data || error.message);\r\n          this.sessionActive = false;\r\n        });\r\n    },\r\n\r\n    // Navigate to the menu page\r\n    goToMenu() {\r\n      window.location.href = '/menu';\r\n    },\r\n\r\n    // Handle Logout (simply redirect or call backend logout endpoint)\r\n    logout() {\r\n      // Optionally, clear the session on the backend if required\r\n      axios.post('http://localhost:5000/api/logout', {}, { withCredentials: true })\r\n        .then(() => {\r\n          this.sessionActive = false;\r\n          this.username = '';\r\n          window.location.href = '/';\r\n        })\r\n        .catch(error => {\r\n          console.error('Logout failed:', error.response?.data || error.message);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* General reset for padding and margin */\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n/* Navbar styles */\r\n.navbar {\r\n  background-color: #ffe3c5;\r\n  padding: 10px 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.nav-icons {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.cart-icon {\r\n  margin-right: 20px;\r\n  font-size: 1.5em;\r\n  color: #000;\r\n  text-decoration: none;\r\n}\r\n\r\n.cart-icon:hover {\r\n  color: #ff6347; /* Example hover color */\r\n}\r\n\r\n.navbar ul {\r\n  list-style: none;\r\n  display: flex;\r\n  gap: 20px;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.navbar li {\r\n  padding: 8px 16px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.navbar li.active {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n.navbar li:hover {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n.navbar li a {\r\n  text-decoration: none;\r\n  color: #6b4226;\r\n}\r\n\r\n/* Logout Button */\r\n.logout {\r\n  border: none;\r\n  padding: 8px 16px;\r\n  font-size: 1em;\r\n  color: #6b4226;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n  text-decoration: none;\r\n}\r\n\r\n.logout:hover {\r\n  background-color: #ffb7a1;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Home Section styles */\r\n.home {\r\n  text-align: center;\r\n  padding: 50px;\r\n  color: #6b4226;\r\n}\r\n\r\n.home h1 {\r\n  font-size: 2.5em;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.home p {\r\n  font-size: 1.2em;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n/* Shop Now button */\r\n.shop-now {\r\n  background-color: #ffb7a1;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  font-size: 1em;\r\n  color: white;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.shop-now:hover {\r\n  background-color: #f28268;\r\n}\r\n\r\n/* Responsive design */\r\n@media screen and (max-width: 768px) {\r\n  .navbar ul {\r\n    flex-direction: column;\r\n    align-items: center;\r\n    gap: 10px;\r\n  }\r\n\r\n  .logout {\r\n    margin-top: 10px;\r\n    width: 100%;\r\n    max-width: 200px;\r\n  }\r\n\r\n  .shop-now {\r\n    width: 100%;\r\n    max-width: 200px;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 480px) {\r\n  .home h1 {\r\n    font-size: 1.8em;\r\n  }\r\n\r\n  .home p {\r\n    font-size: 1em;\r\n  }\r\n\r\n  .shop-now {\r\n    font-size: 0.9em;\r\n    padding: 8px 16px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAK;;EAETA,KAAK,EAAC;AAAQ;;EAgBZA,KAAK,EAAC;AAAW;;EAnB5BC,GAAA;EAwBgCC,IAAI,EAAC;;;EAxBrCD,GAAA;EA0BkBC,IAAI,EAAC,QAAQ;EAACF,KAAK,EAAC;;;EAKzBA,KAAK,EAAC;AAAM;;uBA9BvBG,mBAAA,CAmCM,OAnCNC,UAmCM,GAlCJC,mBAAA,YAAe,EACfC,mBAAA,CAyBM,OAzBNC,UAyBM,G,0BA5BVC,kBAAA,8UAkBMH,mBAAA,iCAAoC,EACpCC,mBAAA,CAQM,OARNG,UAQM,G,0BAPJH,mBAAA,CAEI;IAFDJ,IAAI,EAAC,OAAO;IAACF,KAAK,EAAC;MACpBM,mBAAA,CAAoC;IAAjCN,KAAK,EAAC;EAAsB,IAAMK,mBAAA,2BAA8B,C,sBAErEA,mBAAA,yDAA4D,EACnDK,KAAA,CAAAC,aAAa,I,cAAtBR,mBAAA,CAA+D,KAA/DS,UAA+D,EAAAC,gBAAA,CAAfH,KAAA,CAAAI,QAAQ,oBAxBhET,mBAAA,gBAyBiBK,KAAA,CAAAC,aAAa,I,cAAtBR,mBAAA,CAA0E;IAzBlFF,GAAA;IAyBgCC,IAAI,EAAC,GAAG;IAACF,KAAK,EAAC,QAAQ;IAAEe,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,MAAA,IAAAD,QAAA,CAAAC,MAAA,IAAAF,IAAA,CAAM;KAAE,QAAM,M,cACtEd,mBAAA,CAAgD,KAAhDiB,UAAgD,EAAT,OAAK,G,KAIhDf,mBAAA,kBAAqB,EACrBC,mBAAA,CAIU,WAJVe,UAIU,G,0BAHRf,mBAAA,CAA6B,YAAzB,sBAAoB,sB,0BACxBA,mBAAA,CAA0D,WAAvD,qDAAmD,sBACtDA,mBAAA,CAA4D;IAApDN,KAAK,EAAC,UAAU;IAAEe,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAI,QAAA,IAAAJ,QAAA,CAAAI,QAAA,IAAAL,IAAA,CAAQ;KAAE,UAAQ,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}